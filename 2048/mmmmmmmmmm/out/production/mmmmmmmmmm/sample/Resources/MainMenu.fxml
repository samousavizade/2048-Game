<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseEntered="#onEnterButtonAction" onMouseExited="#onExitButtonAction" prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainMenu">
   <children>
      <Button fx:id="StartGameButtonGameMenu" layoutX="125.0" layoutY="173.0" mnemonicParsing="false" onMouseClicked="#changeToStartGame" onMouseEntered="#onEnterButtonAction" onMouseExited="#onExitButtonAction" prefHeight="250.0" prefWidth="250.0" style="-fx-background-color: rgba(253, 214, 43, 1); -fx-background-radius: 500;" text="Start Game">
         <font>
            <Font name="Comic Sans MS Bold" size="34.0" />
         </font>
      </Button>
      <Button fx:id="LogOutButtonMainMenu" layoutX="25.0" layoutY="374.0" mnemonicParsing="false" onMouseClicked="#changeToEntryMenu" onMouseEntered="#onEnterButtonAction" onMouseExited="#onExitButtonAction" prefHeight="99.0" prefWidth="100.0" style="-fx-background-color: rgba(253, 214, 43, 1); -fx-background-radius: 500;" text="LogOut">
         <font>
            <Font name="Comic Sans MS Bold" size="21.0" />
         </font>
      </Button>
      <Button fx:id="SettingButtonMainMenu" layoutX="375.0" layoutY="374.0" mnemonicParsing="false" onMouseClicked="#changeToSettingMenu" onMouseEntered="#onEnterButtonAction" onMouseExited="#onExitButtonAction" prefHeight="99.0" prefWidth="100.0" style="-fx-background-color: rgba(253, 214, 43, 1); -fx-background-radius: 500;" text="Setting">
         <font>
            <Font name="Comic Sans MS Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="WellComeLabelMainMenu" alignment="CENTER" contentDisplay="CENTER" layoutX="43.0" layoutY="23.0" prefHeight="82.0" prefWidth="414.0" style="-fx-background-radius: 50; -fx-background-color: rgb(253,251,135);" text="fd" textAlignment="CENTER">
         <font>
            <Font name="Segoe Print Bold" size="24.0" />
         </font>
         <effect>
            <Reflection bottomOpacity="0.02" fraction="0.91" topOffset="1.0" topOpacity="0.52" />
         </effect></Label>
   </children>
</AnchorPane>
